    "fixed": ["%if_devel"],
    "dynamic": ["%game_mode in %%levels/city0/game_modes", "%user_count <= %%levels/city0/max_users"]
  },
  "on_execute": {
    "include": "scene_city0.scn.json"
  },
  "on_before_select": {
